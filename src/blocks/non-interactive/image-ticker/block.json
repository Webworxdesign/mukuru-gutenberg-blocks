{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wwx/image-ticker",
	"version": "0.1.0",
	"title": "Image Ticker",
	"category": "widgets",
	"icon": "images-alt",
	"description": "An image ticker block",
	"attributes": {
		"tickerSpeed": {
			"type": "string",
			"default": "10"
		},
		"pauseOnHover": { 
			"type": "boolean",
			"default": true
		}
	}, 
	"textdomain": "image-ticker", 
	"supports": {
		"interactivity": true,
		"align": ["full", "wide"]
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.js"
}