{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wwx/image-tooltips",
	"version": "0.1.0",
	"title": "Image Tooltips",
	"category": "widgets",
	"icon": "media-interactive",
	"description": "An interactive image tooltips block with the Interactivity API",
	"example": {},
	"supports": {
		"interactivity": true
	},
	"attributes": {  
		"style": {
			"type": "string", 
			"default": "default"
		},
        "tooltipsImage": {
            "type": "string",
            "default": ""
        },
        "tooltips": {
            "type": "array",
            "default": []
        }
	},
	"textdomain": "image-tooltips",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.js"
}
